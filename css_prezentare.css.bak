@font-face {
  font-family: 'FrizQuadrataRegular';
  src: url('Friz Quadrata Regular.ttf');
}
p {
	   position: relative;
	   padding: 10px;
	   font-family: 'FrizQuadrataRegular';
       margin: 5%;
